{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    // "target": "esnext",
    "module": "commonjs",
    "target": "esnext", // Or "es2015" or later
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "include": [
    "./*.js",
    "src/**/*.ts",
    "test/**/*.ts"
, "src/api/sid-microsites/sid-microsites.error.ts"  ],
}
