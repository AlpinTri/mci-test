{
  "name": "mci-core",
  "version": "1.5.12",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "lint": "eslint --fix src test",
    "test": "jest",
    "image": "./build.sh",
    "image:stage": "./build.sh stage",
    "typecheck": "tsc --noEmit",
    "mikro-orm": "mikro-orm",
    "seeder:create": "MIKRO_ORM_SEEDER_PATH=src/seeders mikro-orm seeder:create",
    "seeder:run": "MIKRO_ORM_SEEDER_PATH=dist/seeders mikro-orm seeder:run",
    "migration:create": "mikro-orm migration:create -p './src/migrations'",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:list": "mikro-orm migration:list",
    "migration:pending": "mikro-orm migration:pending",
    "deploy": "npm run build && pm2 start npm -- start"
  },
  "keywords": [],
  "author": "izzyes",
  "repository": {
    "type": "git",
    "url": "https://github.com/Media-Cloud-Indonesia/mci-core.git"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@cpanel/api": "^5.1.2",
    "@mikro-orm/cli": "^5.7.14",
    "@mikro-orm/core": "^5.7.14",
    "@mikro-orm/migrations": "^5.7.14",
    "@mikro-orm/postgresql": "^5.7.14",
    "@mikro-orm/seeder": "^5.7.14",
    "@penseapp/discord-notification": "^2.0.9",
    "@sendgrid/mail": "^7.7.0",
    "@supabase/supabase-js": "^2.42.5",
    "@types/mjml": "^4.7.4",
    "axios": "^1.6.2",
    "bcrypt": "5.1.0",
    "cors": "^2.8.5",
    "country-state-city": "^3.2.1",
    "currency-codes": "^2.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-ipfilter": "^1.3.1",
    "firebase": "^10.3.0",
    "generate-password": "^1.7.1",
    "googleapis": "^131.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.10.1",
    "joi-password-complexity": "^5.2.0",
    "js-base64": "^3.7.6",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mjml": "^4.14.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^11.7.0",
    "node-cron": "^3.0.2",
    "read-excel-file": "^5.7.1",
    "rimraf": "^5.0.1",
    "short-uuid": "^4.2.2",
    "socks-proxy-agent": "^8.0.2",
    "util": "^0.12.5",
    "utility-types": "^3.11.0",
    "uuid": "^9.0.0",
