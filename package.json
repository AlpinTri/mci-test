    "@types/morgan": "^1.9.4",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.3",
    "@types/node-cron": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
